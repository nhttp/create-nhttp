{
  "tasks": {
    "dev": "deno run -A --watch main.ts",
    "start": "deno run -A main.ts"
  },
  "imports": {
    "nhttp": "https://deno.land/x/nhttp@<VERSION>/mod.ts",
    "nhttp/jsx-runtime": "https://deno.land/x/nhttp@<VERSION>/lib/jsx/jsx-runtime.ts",
    "nhttp/": "https://deno.land/x/nhttp@<VERSION>/lib/",
    "models/": "./models/",
    "views/": "./views/",
    "controllers/": "./controllers/",
    "components/": "./components/"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "nhttp"
  }
}
