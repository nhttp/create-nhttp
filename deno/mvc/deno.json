{
  "tasks": {
    "dev": "deno run -A --watch main.ts",
    "start": "deno run -A main.ts"
  },
  "imports": {
    "@nhttp/nhttp": "jsr:@nhttp/nhttp@<VERSION>",
    "models/": "./models/",
    "views/": "./views/",
    "controllers/": "./controllers/",
    "components/": "./components/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@nhttp/nhttp/jsx",
    "experimentalDecorators": true,
    "lib": [
      "deno.window",
      "dom"
    ]
  }
}
