{
  "name": "preact-ssr",
  "version": "1.0.0",
  "description": "preact-ssr template for nhttp",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "tsx ./_core/dev.ts && tsx watch --clear-screen=false main.ts --dev",
    "build": "tsx --no-cache ./_core/gen.ts && tsx --no-cache ./_core/build.ts",
    "start": "tsx --no-cache main.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "nhttp-land": "^<VERSION>",
    "preact": "^10.17.1",
    "preact-render-to-string": "^6.2.1"
  },
  "devDependencies": {
    "tsx": "^3.12.7"
  }
}
