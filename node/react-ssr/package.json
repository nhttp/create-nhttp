{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "react-ssr template for nhttp",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "tsx ./_core/dev.ts && tsx watch --clear-screen=false main.ts --dev",
    "build": "tsx --no-cache ./_core/gen.ts && tsx --no-cache ./_core/build.ts",
    "start": "tsx --no-cache main.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "nhttp-land": "^<VERSION>",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "tsx": "^3.12.7"
  }
}
